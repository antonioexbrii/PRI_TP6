extends layout
block content
	.w3-container.w3-card-4
		header.w3-container.w3-light-blue
			h2 Tese adicionada com sucesso!
		.w3-container.light-grey
			table.w3-table-all
					tr
						th Nome
						td= novo.nome
					tr
						th N. Auno
						td= novo.mecan
					tr
						th Curso
						td= novo.curso
					tr
						th Instituição
						td= novo.inst
					tr
						th Título
						td= novo.titulo
					tr
						th Data
						td= novo.data
					tr
						th Grau de Ensino
						td= novo.tipo
			br
			form.w3-container(action='/',method='GET')
				input.w3-button.w3-block(type='submit',value='Voltar ao Menu Inicial')
		footer.w3-container.w3-light-blue
			address PRI2018 - António Chaves A75870	